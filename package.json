    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^3.1.0",
    "memory-fs": "^0.5.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
